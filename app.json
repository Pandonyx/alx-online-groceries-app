{
  "expo": {
    "name": "alx-online-groceries-app",
    "slug": "alx-online-groceries-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "fonts": [
      "src/assets/fonts/Gilroy-Regular.ttf",
      "src/assets/fonts/Gilroy-Medium.ttf",
      "src/assets/fonts/Gilroy-SemiBold.ttf",
      "src/assets/fonts/Gilroy-Bold.ttf",
      "src/assets/fonts/Gilroy-Heavy.ttf",
      "src/assets/fonts/Gilroy-Light.ttf",
      "src/assets/fonts/Gilroy-Thin.ttf"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
